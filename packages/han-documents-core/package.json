{
  "name": "@han/core",
  "version": "0.1.0",
  "type": "module",
  "description": "Shared types, utilities and constants for Han Documents builders",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist/**/*",
    "README.md"
  ],
  "scripts": {
    "build": "tsc && vite build",
    "dev": "tsc --watch",
    "lint": "eslint src --ext .ts",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "clean": "rimraf dist"
  },
  "keywords": [
    "documents",
    "excel",
    "word",
    "pdf",
    "types",
    "utilities"
  ],
  "author": "Han Documents Team",
  "license": "MIT",
  "devDependencies": {
    "typescript": "^5.0.2",
    "vite": "^4.5.2",
    "vite-plugin-dts": "^3.6.3",
    "vitest": "^1.0.0",
    "rimraf": "^5.0.0"
  }
}

